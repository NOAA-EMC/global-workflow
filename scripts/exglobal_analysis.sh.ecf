   cp $SIGANL  $COMOUT/${RUN}.${cycle}.sanl
   cp $ABIAS   $COMOUT/${RUN}.${cycle}.abias
   cp $ABIASPC $COMOUT/${RUN}.${cycle}.abias_pc
fi

