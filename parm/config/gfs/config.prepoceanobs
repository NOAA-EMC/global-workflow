#!/bin/bash

########## config.prepoceanobs ##########

echo "BEGIN: config.prepoceanobs"

export OCNOBS2IODAEXEC=${HOMEgfs}/sorc/gdas.cd/build/bin/gdas_obsprovider2ioda.x

export SOCA_INPUT_FIX_DIR=@SOCA_INPUT_FIX_DIR@

export OBS_YAML_DIR="${PARMgfs}/gdas/soca/obs/config"
